package javaQuestionPractise;

public class Primenumbersbetween1to50
{

	public static void main(String[] args)
	{
			//2,3,5,7,11,13,17,19
		int lastnum=50;
	
		int n=2;
		System.out.println("n = "+n);
		for(int i=1;i<=lastnum;i++)
		{
			int count=0;
			for(int j=1;j<=n;j++)
			{
				if(n%j==0)
				{
					count++;
				}
				if(count==2)
				{
					System.out.println(n+"is a prime");
				}
				n++;
			}
		}
	}

}
